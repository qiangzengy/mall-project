{
  "query": {
    "bool": {
      "must": [
        {"match": {
          "skuTitle": "华为"
        }}
      ],
      "filter": [
        {
          "term": {
            "catalogId": "225"
          }
        },
        {
          "terms": {
            "brandId": [
              "1",
              "2",
              "9"
            ]
          }
        },
        {
          "nested": {
            "path": "attrs",
            "query": {
              "bool": {
                "must": [
                  {"term": {
                    "attrs.attrId": {
                      "value": "15"
                    }
                  }
                  },
                  {
                    "terms": {
                      "attrs.attrValue": [
                        "海思",
                        "高通"
                      ]
                    }
                  }
                ]
              }
            }
          }
        },
        {
          "term": {
            "hasStock": "true"
          }
        },
        {
          "range": {
            "skuPrice": {
              "gte": 0,
              "lte": 6499
            }
          }
        }
      ]
    }
  },
}